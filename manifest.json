{
  "manifest_version": 2,
  "name": "Tab Find",
  "description": "Any Description",
  "short_name": "Tab Find",
  "version": "0.0.1",
  "author": "Adriano Chambel M. de Lima <acmlima.softweb@gmail.com>",
  "homepage_url": "https://github.com/achambel/tab-find",
  "minimum_chrome_version": "55",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Tab Find",
    "default_popup": "popup.html"
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "mac": "Command+Shift+5"
      },
      "description": "Toggle feature foo",
      "global": true
    }
  }
}