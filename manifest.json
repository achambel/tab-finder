{
  "manifest_version": 2,
  "name": "Tab Finder",
  "description": "Any Description",
  "short_name": "Tab Finder",
  "version": "0.0.1",
  "author": "Adriano Chambel M. de Lima <acmlima.softweb@gmail.com>",
  "homepage_url": "https://github.com/achambel/tab-finder",
  "minimum_chrome_version": "55",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_title": "Open Tab Finder | Ctrl+Shift+9",
    "default_popup": "popup.html"
  },
  "commands": {
    "open-query-form": {
      "suggested_key": {
        "default": "Ctrl+Shift+9"
      },
      "description": "Open Tab Finder",
      "global": true
    }
  },
  "icons": {
    "16": "img/16x16.png",
    "48": "img/48x48.png",
    "128": "img/128x128.png"
  }
}